  <!-- Build options -->
  <build>
    <option directives="openfl=3.3.7&#xA;lime=2.6.7&#xA;actuate=1.8.6&#xA;swf=2.1.0&#xA;openfl-next&#xA;tools=2.6.7&#xA;no-compilation&#xA;native&#xA;lime-native&#xA;openfl-native&#xA;windows&#xA;desktop&#xA;display" />
