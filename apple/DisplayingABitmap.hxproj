  <!-- Other classes to be compiled into your SWF -->
  <classpaths>
    <class path="C:\HaxeToolkit\haxe\lib\openfl\3,3,7" />
    <class path="C:\HaxeToolkit\haxe\lib\lime\2,6,7" />
    <class path="C:\HaxeToolkit\haxe\lib\actuate\1,8,6" />
    <class path="C:\HaxeToolkit\haxe\lib\swf\2,1,0" />
    <class path="Source" />
    <class path="Export\windows\cpp\haxe" />
